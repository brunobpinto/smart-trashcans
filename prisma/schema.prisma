// Prisma Schema Configuration
generator client {
  provider   = "prisma-client"
  output     = "../generated/prisma"
  engineType = "client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Enums for the project
enum BinType {
  COMMON
  RECYCLE
}

enum Role {
  WORKER
  ADMIN
}
